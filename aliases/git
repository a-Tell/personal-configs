alias gs='git status'
alias gp='git pull'
alias ga='git add -A'
alias gps='git push'
alias gca='git commit -a'
alias gaca='git add -A && git commit -a'

# Yolo's
alias gniceyolo='git commit -am "Just nice yolo-pushing..." && git push'
alias gyolo='git commit -am "Just yolo-pushing..." && git push -f'
